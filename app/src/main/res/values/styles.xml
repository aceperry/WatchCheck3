<resources>

    <style name="ListHeadline" parent="Base.TextAppearance.AppCompat.SearchResult.Title">
        <item name="android:textSize">11dp</item>
    </style>

    <style name="SmallListItem" parent="Base.TextAppearance.AppCompat.Small">
        <item name="android:textSize">10dp</item>
    </style>

    <style name="SmallListItemCenter" parent="SmallListItem">
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="MediumListItem" parent="Base.TextAppearance.AppCompat.Small">
    </style>

    <style name="spinnerItemStyle" parent="Base.Widget.AppCompat.Spinner">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="checkboxItemStyle" parent="">
        <item name="colorAccent">@color/textColorPrimary</item>
        <item name="colorPrimary">@color/textColorPrimary</item>
    </style>




</resources>
